##########################################################
# Contains all properties variables needed in the plugin #
#                                                        #
#       If you want use a default value,                 #
#       please comment out the hole line!                #        
##########################################################

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
#          Name of the UWE profile file             #
# First, MagicUWE searches for the UWE-Profile in   # 
# the directory of the actual project.              #
# If this file does not exist, the one in the       # 
# profile directory of MagicDraw will be chosen.    #
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

# name of the UWE profile file in the directory of the actual project
uweProfileNameInTheProjectDirectory=UWE Profile.mdzip

# name of the UWE profile file in the MagicDraw profile directory
uweProfileName=UWE Profile.mdzip

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
#  Use of an submenu called "UWE" for the           #
# "new Diagram" contextmenu of the containment tree #
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

# default: true
useNewSubmenu=true

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
#  MagicUWE4SDE							            #
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

# default: true
SDEport=4001

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
# Shortcuts for the UWE Element Toolbar Buttons    #
#    (only use letters -upper letters- here,       #
#             "null" means no shortcut).           #
# You must press Alt+Shift+ThatLetter in MagicDraw.#
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

# Navigation Model
keynavigationClass=K
keyguidedTour=U
keyindex=I
keyquery=O
keymenu=P
keyexternalNode=null

# Navigation State Model
keynavigationalNode=N
keysession=S
keycollection=C
keytarget=T
keyexternalNode=E

# Transitions
keyintegratedMenu=M
keysearch=Q
keyallItems=A

# Associations
keynavigationLink=Y
keydirectednavigationLink=N
keyprocessLink=X
keydirectedprocessLink=M

# Presentation Model Classes
keyclassinputForm=null
keyclassimage=null
keyclassbutton=null
keyclasspresentationGroup=Z
keyclasspresentationPage=H
keyclassanchor=null
keyclasstext=null
keyclasstextInput=null
keyclassselection=null
keyclasscustomComponent=null
keyclassfileUpload=null
keyclassmediaObject=null
keyclassiteratedPresentationGroup=null
keyclasspresentationAlternatives=null

# Presentation Model Properties
keypropertypresentationGroup=T
keypropertypresentationPage=null
keypropertyinputForm=W
keypropertyimage=E
keypropertybutton=R
keypropertyanchor=S
keypropertytext=F
keypropertytextInput=G
keypropertyselection=null
keypropertycustomComponent=null
keypropertyfileUpload=null
keypropertymediaObject=null
keypropertyiteratedPresentationGroup=null
keypropertypresentationAlternatives=null

# Process Class (used in Navigation and Process Diagram)
keyprocessClass=V

# Process Flow
keysystemAction=S
keyuserAction=U

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#
#         Set in which toolbar menu the            #
#        presentation element is displayed         #
#         (only use true or false here).           #
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~#

# Show class Toolbar Buttons
inputform=false
image=false
button=false
presentationGroup=true
presentationPage=true
anchor=false
text=false
textInput=false
selection=false
customComponent=false
fileUpload=false
mediaObject=false
iteratedPresentationGroup=true
presentationAlternatives=false


# Show property Toolbar Buttons
inputformProperty=true
imageProperty=true
buttonProperty=true
presentationGroupProperty=true
presentationPageProperty=false
anchorProperty=true
textProperty=true
textInputProperty=true
selectionProperty=true
customComponentProperty=true
fileUploadProperty=true
mediaObjectProperty=true
iteratedPresentationGroupProperty=true
presentationAlternativesProperty=true


